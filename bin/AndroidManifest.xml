        </activity>
        <activity
            android:name=".MakeRoom"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".SeachRoom"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Warikan"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".AllRoom"
            android:label="@string/title_activity_all_room" >
        </activity>
        <activity
            android:name=".MemberRoom"
            android:label="@string/title_activity_member_room" >
        </activity>
        <activity
            android:name=".KanjiRoom"
            android:label="@string/title_activity_kanji_room" >
        </activity>
        <activity
            android:name=".RoomHensyu"
            android:label="@string/title_activity_room_hensyu" >
        </activity>
    </application>

</manifest>
